def add_operator(self, operator):
        check_ttl = []
        
        for y in self.expression:
        	check_ttl.append(y)
        		   
        if check_ttl[0] == operator:
        	pass
        self.expression += operator
        self.total += self.expression
        self.expression = ""
        self.update_label()
        self.update_total()

